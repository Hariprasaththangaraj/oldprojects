package com.server.main.user.practise;

public class javapractise {

	public static void main(String args[]) {

		practisemethod();

	}

	public static void practisemethod() {
		// String reverse

		String name = "Hariprasath";

		char[] print = name.toCharArray();
		int charlength = print.length;
		
		/*
		 * for (int i = print.length-1; i >= 0; i--) { System.out.print(print[i]); }
		 */

		System.out.print('\n'+"LEFT ROTATION  : ");
		
		for(int i = charlength-3; i >=0; i --) {
				System.out.print(print[i]);
		}
		
	}
}
